{
  "name": "widget-generator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "devAVP": "XR_ENV=avp vite",
    "sim:dev": "npx webspatial-builder run --base=http://localhost:5173/ ",
    "build": "XR_ENV=avp tsc -b && XR_ENV=avp vite build",
    "sim:distpkg": "npx webspatial-builder run",
    "build:sim:distpkg": "npm run build && npm run sim:distpkg",
    "ipa:distpkg": "dotenv -e .env.local -- sh -c 'webspatial-builder build --base=/ --teamId=$APPLE_TEAM_ID --bundle-id=$BUNDLE_ID'",
    "publish:ipa:distpkg": "dotenv -e .env.local -- sh -c 'webspatial-builder publish --base=/ --teamId=$APPLE_TEAM_ID --bundle-id=$BUNDLE_ID --version=$VERSION --u=$USERNAME --p=$PASSWORD'",
    "build:ipa:distpkg": "npm run build && npm run ipa:distpkg",
    "build:publish": "npm run build && npm run publish:ipa:distpkg",
    
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@google/model-viewer": "^4.1.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@tailwindcss/vite": "^4.1.11",
    "@webspatial/builder": "^1.0.3",
    "@webspatial/core-sdk": "^1.0.3",
    "@webspatial/platform-visionos": "^1.0.3",
    "@webspatial/react-sdk": "^1.0.3",
    "@webspatial/vite-plugin": "latest",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.0",
    "lucide-react": "^0.503.0",
    "pixi.js": "^8.11.0",
    "react": "^19.1.0",
    "react-day-picker": "8.10.1",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "three": "^0.176.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^22.16.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "tw-animate-css": "^1.3.5",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.36.0",
    "vite": "^6.3.5"
  }
}
